{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"customTemplates": [
		{
			"name": "template-page-w-hero",
			"title": "Page with Hero"
		},
		{
			"name": "template-page-w-title",
			"title": "Page with Title"
		}
	],
	"settings": {
		"appearanceTools": true,
		"blocks": {
			"core/heading": {
				"color": {
					"background": false
				}
			}
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [
				{
					"colors": [
						"#000000",
						"#FFFFFF"
					],
					"name": "Duotone 1",
					"slug": "duotone-1"
				},
				{
					"colors": [
						"#000000",
						"#BBBBBB"
					],
					"name": "Duotone 2",
					"slug": "duotone-2"
				}
			],
			"palette": [
				{
					"color": "#f5f1f0",
					"name": "Color 1",
					"slug": "theme-1"
				},
				{
					"color": "#a58e86",
					"name": "Color 2",
					"slug": "theme-2"
				},
				{
					"color": "#333333",
					"name": "Color 3",
					"slug": "theme-3"
				},
				{
					"color": "#131212",
					"name": "Color 4",
					"slug": "theme-4"
				},
				{
					"color": "#000000",
					"name": "Color 5",
					"slug": "theme-5"
				}
			]
		},
		"custom": {
			"spacing-increment": "2.5",
			"spacing-unit": "10"
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1440px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingScale": {
				"theme": {
					"steps": 1
				}
			},
			"spacingSizes": [
				{
					"name": "2X-Small",
					"size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 1) * 1px), calc(var(--wp--custom--spacing-increment) * 1 * 2vw))",
					"slug": "20"
				},
				{
					"name": "X-Small",
					"size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 2) * 1px), calc(var(--wp--custom--spacing-increment) * 2 * 1vw))",
					"slug": "30"
				},
				{
					"name": "Small",
					"size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 3) * 1px), calc(var(--wp--custom--spacing-increment) * 3 * 1vw))",
					"slug": "40"
				},
				{
					"name": "Medium",
					"size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 4) * 1px), calc(var(--wp--custom--spacing-increment) * 4 * 1vw))",
					"slug": "50"
				},
				{
					"name": "Large",
					"size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 5) * 1px), calc(var(--wp--custom--spacing-increment) * 5 * 1vw))",
					"slug": "60"
				},
				{
					"name": "Extra Large",
					"size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 6) * 1px), calc(var(--wp--custom--spacing-increment) * 6 * 1vw))",
					"slug": "70"
				},
				{
					"name": "2X Large",
					"size": "min(calc(var(--wp--custom--spacing-unit) * (var(--wp--custom--spacing-increment) * 7) * 1px), calc(var(--wp--custom--spacing-increment) * 7 * 1vw))",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Gabarito",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 900",
							"src": [
								"file:./assets/fonts/gabarito/Gabarito-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Gabarito\", sans-serif",
					"name": "Gabarito",
					"slug": "gabarito"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Ibarra Real Nova",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "100 800",
							"src": [
								"file:./assets/fonts/ibarra-real-nova/IbarraRealNova-VariableFont_wght.woff2"
							]
						}
					],
					"fontFamily": "\"Ibarra Real Nova\", serif",
					"name": "Ibarra Real Nova",
					"slug": "ibarra-real-nova"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "20px",
						"min": "19px"
					},
					"name": "Small",
					"size": "20px",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "18px",
						"min": "16px"
					},
					"name": "Medium",
					"size": "18px",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "28px",
						"min": "24px"
					},
					"name": "Large",
					"size": "28px",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "54px",
						"min": "40px"
					},
					"name": "Extra Large",
					"size": "54px",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "64px",
						"min": "44px"
					},
					"name": "2X Large",
					"size": "64px",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "140px",
						"min": "40px"
					},
					"name": "3X Large",
					"size": "140px",
					"slug": "xxx-large"
				}
			],
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
